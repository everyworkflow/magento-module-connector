<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">EveryWorkflow - Magento Admin Panel</argument>
        </action>
    </referenceBlock>
    <body>
        <referenceContainer name="content">
            <block class="EveryWorkflow\Connector\Block\Adminhtml\MageAdmin"
                   template="EveryWorkflow_Connector::mage_admin.phtml"/>
        </referenceContainer>
    </body>
</page>
